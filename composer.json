{
  "name": "zver/package-template",
  "description": "this is universal package template for development and testing",
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    }
  },
  "require": {
    "zver/string-helper": "^2.0",
    "zver/array-helper": "^1.0"
  },
  "require-dev": {
  }
}
