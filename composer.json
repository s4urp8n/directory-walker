{
  "name": "zver/directory-walker",
  "description": "this is universal package template for development and testing",
  "minimum-stability": "dev",
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests/"
    }
  },
  "require": {
    "phpunit/phpunit": "*"
  },
  "require-dev": {
  }
}
